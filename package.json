{
  "name": "gleamy",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "publish:package": "turbo run publish:package",
    "dry-run": "turbo run dry-run",
    "prettier": "prettier --config .prettierrc.json './**/*.{ts,tsx,js,json}' --write",
    "test": "turbo run test",
    "test:watch": "pnpm test --watch",
    "lint": "turbo run lint"
  },
  "engines": {
    "node": ">=20.15.1",
    "pnpm": ">=9.5.0"
  },
  "packageManager": "pnpm@9.5.0",
  "dependencies": {
    "@manypkg/cli": "0.21.4",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4",
    "turbo": "2.0.11"
  }
}